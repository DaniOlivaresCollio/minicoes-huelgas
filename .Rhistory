aes(label = ifelse(porcentaje > 0 , scales::percent(porcentaje, accuracy = .1),"")),
position = position_stack(vjust = .5),
show.legend = FALSE,
size = 4,
color = rep('white'),
bg.colour='grey30')+
scale_x_discrete(labels = c(2016, 2017, 2018, 2019, 2021, 2022, 2023))
alluvial_clase
